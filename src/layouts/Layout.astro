---
import Meta from '../meta.astro';
import Header from '../sections/Header.astro';
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <Meta title={title} />
  </head>

  <body class='bg-white-snow'>
    <Header />
    <slot />
  </body>
</html>


<style is:global>
  @import url("https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&family=Roboto&display=swap");
</style>

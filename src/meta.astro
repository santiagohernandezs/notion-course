---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<link rel="icon" type="image/svg+xml" href="/notion.svg" />
<title>{title}</title>

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width" />
<meta name="generator" content={Astro.generator} />
<meta name="theme-color" content="#121212" />

<meta
  name="description"
  content="Curso de notion para creativos que quieren organizar su vida y su trabajo. Aprende a usar notion en tu vida y sácale el mayor partido"
/>

<meta property="og:title" content="Notion Venezuela - Notion para creativos." />
<meta
  property="og:description"
  content="Curso de notion para creativos que quieren organizar su vida y su trabajo. Aprende a usar notion en tu vida y sácale el mayor partido"
/>
<meta property="og:type" content="website" />
<meta property="og:locale" content="es_VE" />
<meta property="og:locale:alternate" content="es_ES" />
<meta property="og:determiner" content="de" />

<meta
  property="og:image"
  content="https://opengraph.githubassets.com/00bdf7756115de74b90dddb0879d7a41bd40f3a402853407ee8b45eba3dff5c3/RafidMuhymin/astro-imagetools"
/>
<meta property="og:image:type" content="image/png" />
<meta
  property="og:image:alt"
  content="Imagen de la comunidad de Notion Venezuela."
/>
<meta property="og:image:width" content="1200" />
<meta property="og:image:height" content="630" />

<meta name="twitter:card" content="summary_large_image" />
<meta property="twitter:domain" content="github.com" />
